// A Test is a test model.
model TestModel: "/tests/{test_id}" {
	// The name.
	string name;
    // The age.
	integer age;
    // The number pi.
    double pi;
    // The birth date.
    timestamp birthdate;
    // True if it is good.
    boolean is_good;

    bytes data;

    reference friend;
    geopoint location;
    array colors;
    map meta;

	// Fake types...
	URL anURL;
}
